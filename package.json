{
  "name": "harmoney",
  "version": "1.0.0",
  "description": "Holistic Accounts Management System",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha ",
    "test:report": "nyc mocha",
    "start": "cross-env NODE_ENV=dev node dist/bin/www",
    "build": "npm run compile:all",
    "postbuild": "npm run start",
    "prebuild": "npm-run-all test clean",
    "postinstall": "rimraf ./dist && npm run compile:all",
    "clean": "rimraf ./dist && mkdir dist",
    "start:www": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "start:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "compile:models": "babel ./models --out-dir dist/models",
    "compile:server": "babel ./server.js --out-dir dist/",
    "compile:controlers": "babel ./controllers --out-dir dist/controllers",
    "compile:db": "babel ./db --out-dir dist/db",
    "compile:middleware": "babel ./middleware --out-dir dist/middleware",
    "compile:routes": "babel ./routes --out-dir dist/routes",
    "compile:services": "babel ./services --out-dir dist/services",
    "compile:test": "babel ./test --out-dir dist/test",
    "compile:utilities": "babel ./utilities --out-dir dist/utilities",
    "compile:copy": "ncp ./node_modules/ ./dist/node_modules && ncp ./bin/ ./dist/bin && ncp ./public/ ./dist/public && ncp ./views/ ./dist/views",
    "compile:all": "npm-run-all compile:models compile:controlers compile:db compile:middleware compile:routes compile:services compile:test compile:utilities compile:copy compile:server",
  },
  "author": "Kevin O'Keeffe",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.10",
    "twilio": "^3.40.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-watch": "^6.0.1",
    "http": "0.0.0",
    "http-errors": "^1.7.3",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.0.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "request": "^2.81.0",
    "rimraf": "^3.0.1",
    "supertest": "^4.0.2",
    "validator": "^12.2.0"
  }
}
